{
    "name": "kemodev/laravel-restful-filter",
    "description": "A simple way to filter your query on RESTful API",
    "keywords": [
        "laravel",
        "eloquent",
        "model",
        "restful",
        "search",
        "query",
        "filter"
    ],
    "homepage": "https://github.com/game5413/laravel-restful-filter",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "M Febryan",
            "email": "febryan@kemodev.my.id",
            "homepage": "https://kemodev.my.id",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kemodev\\RestfulFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "php": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}
